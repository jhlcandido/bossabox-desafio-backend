{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "mkdir ./dist && sucrase ./src -d ./dist/src --transforms typescript,imports && cp ./package.json ./dist && cp ./.env ./dist && cp -R ./src/config ./dist/src && cp -R ./src/entities ./dist/src && cp ./.sequelizerc ./dist",
    "startProd": "cross-env NODE_ENV=prod node ./dist/src/server.js",
    "start": "cross-env NODE_ENV=prod tsnd --inspect --transpile-only --respawn --ignore-watch node_modules src/server.ts",
    "dev": "cross-env NODE_ENV=dev tsnd --inspect --transpile-only --respawn --ignore-watch node_modules src/server.ts",
    "migrations:run": "npx sequelize db:migrate",
    "test": "cross-env NODE_ENV=test npm run test:run",
    "test:run": "jest",
    "swagger-autogen": "node src/swagger.ts"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.1.0",
    "sequelize": "^6.3.4",
    "swagger-autogen": "^1.2.0",
    "swagger-ui-express": "^4.1.4",
    "ts-node-dev": "^1.0.0-pre.60",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.0.27",
    "@types/sequelize": "^4.28.9",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^13.1.0",
    "babel-jest": "^26.3.0",
    "jest": "^26.4.2",
    "sequelize-cli": "^6.2.0",
    "sqlite3": "^5.0.0",
    "sucrase": "^3.15.0",
    "ts-jest": "^26.2.0"
  }
}
